{
  "name": "storeon-choo",
  "version": "0.2.1",
  "description": "A tiny (81 bytes) connector for Storeon and Choo",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:distolma/storeon-choo.git",
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "jest --coverage && yarn lint && size-limit"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "81 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config",
    "rules": {
      "node/no-unpublished-require": "off",
      "func-style": "off"
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "peerDependencies": {
    "choo": "^7.1.0",
    "storeon": ">=2.0.0"
  },
  "devDependencies": {
    "@logux/eslint-config": "^39.0.3",
    "@size-limit/preset-small-lib": "^4.5.1",
    "dual-publish": "^0.10.5",
    "eslint": "^7.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.16.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-prettierx": "^0.12.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^20.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "nanobus": "^4.4.0",
    "size-limit": "^4.5.1",
    "storeon": "^3.1.1"
  }
}
